{
  "name": "dictionary-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prod:build": "ng build --prod",
    "xi18n": "ng xi18n --locale en --i18n-format xlf --output-path src/locale",
    "xi18n-all": "npm-run-all xi18n:ng-xi18n xi18n:xliff-to-tpl-xliff xi18n:xliff-merge-to-lang",
    "xliff-to-tpl-xliff": "ts-node scripts/i18n/xliff-to-tpl-xliff.ts",
    "xliff-merge-to-lang": "ts-node scripts/i18n/xliff-merge-to-lang.ts",
    "build:client:en": "ng build --aot --locale en --base-href /en/ --i18nFile=./src/locale/messages.en.xlf --missing-translation ignore --i18nFormat=xlf --output-path ./dist/en",
    "build:client:bg": "ng build --aot --locale bg --base-href /bg/ --i18nFile=./src/locale/messages.bg.xlf --i18nFormat=xlf --output-path ./dist/bg"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^5.0.3",
    "@angular/compiler": "^5.0.3",
    "@angular/core": "^5.0.3",
    "@angular/forms": "^5.0.3",
    "@angular/http": "^5.0.3",
    "@angular/platform-browser": "^5.0.3",
    "@angular/platform-browser-dynamic": "^5.0.3",
    "@angular/router": "^5.0.3",
    "angular2-jwt": "^0.2.3",
    "bootstrap": "^4.0.0-beta.2",
    "core-js": "^2.5.0",
    "ngx-bootstrap": "^2.0.0-beta.9",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.4",
    "@angular/compiler-cli": "^5.0.3",
    "@types/jasmine": "ts2.4",
    "@types/node": "ts2.4",
    "codelyzer": "^4.0.1",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.2.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "~2.4.2",
    "xmldom": "https://github.com/smoke/xmldom/tarball/smoke-xmldom/0.1.28-p1"
  }
}
